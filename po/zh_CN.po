msgid "Could not parse HTTP request"
msgstr "无法解析 HTTP 请求"

#: libsoup/soup-request.c:141
#, c-format
msgid "No URI provided"
msgstr "未提供 URI"

#: libsoup/soup-request.c:151
#, c-format
msgid "Invalid “%s” URI: %s"
msgstr "无效的“%s”URI：%s"

