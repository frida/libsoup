#: libsoup/soup-body-input-stream.c:459
msgid "Invalid seek request"
msgstr "无效的 seek 请求"

#: libsoup/soup-body-input-stream.c:487
msgid "Cannot truncate SoupBodyInputStream"
msgstr "无法截断（truncate）SoupBodyInputStream"

